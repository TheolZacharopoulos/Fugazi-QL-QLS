//form test
form FORMNAME {

    NAME STRING "What is your name?"
    RESULT STRING "Your name: "(NAME)

    NAME1 BOOLEAN "Are you a programmer?"
    if(!NAME1){
        NAME2 BOOLEAN "You should be!"
    }else{
        NAME3 BOOLEAN "Great!"
    }

    NAME4 STRING "text text text"
    COMPUTED STRING "computed:" (1+5*6+3)
    NAME5 STRING "blabla"
    NAME6 BOOLEAN "check"
    NAME7 STRING "not in qls"
}
