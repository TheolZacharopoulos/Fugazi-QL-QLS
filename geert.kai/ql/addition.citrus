grammar Addition
  rule forms
    form*
  end

  rule form
    space 'form' space '{' space query* space '}' space
  end

  rule space
    [ \t\n]*
  end

  rule query
    space text space '{' space answer* space '}' space
  end

  rule answer
    variable ":" space type
  end

  rule type
    'integer' | 'boolean' | 'string'
  end

  rule variable
    [a-zA-Z?]+
  end

  rule text
    /"(.*)"/
  end
end

