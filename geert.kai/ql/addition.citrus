grammar Addition
  rule indention(level)
    '\n' & '  '*level
  end

  rule text(level)
    level '""' 
  end

  rule texts(level)
    text(level)*
  end

  rule form(level)
    level 'form' texts(level "  ")
  end

  rule forms(level)
    form(level)*
  end

  rule additive
    forms("")
  end
end

