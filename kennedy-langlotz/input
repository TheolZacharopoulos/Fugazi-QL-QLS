question
    id : question1
    text : "What is your age?"
    type : numeral
end

if (question1  > 18) then
    question
        id : question2
        text : "When is your birthday?"
        type : date
    end
end

if (question2 > 01.01.1111) then
    question
        id : question3
        text : "What is your name"
        type : string
    end
end

if (question1 > 20 && question3 == "Timon") then
    question
        id : question4
        text : "Why are you so awesome?"
        type : string
    end
end

question
    id : question5
    text : "Hi my name is Jeff!"
    type : string
end